# Frontend Environment Variables for ECS
# Copy this to frontend.env and update with your actual values
# Upload to S3: aws s3 cp frontend.env s3://oneclicktag-env-store-{env}/frontend.env

# API Configuration
VITE_API_URL=https://dev-api.oneclicktag.com/api
VITE_API_TIMEOUT=30000

# Firebase (optional)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Google OAuth (optional)
VITE_GOOGLE_CLIENT_ID=

# App Configuration
VITE_APP_NAME=OneClickTag
VITE_APP_URL=https://dev.oneclicktag.com

# Feature Flags (optional)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
