{
  "buildCommand": "pnpm prisma generate --schema=./prisma/schema.prisma && pnpm run build",
  "outputDirectory": "dist",
  "functions": {
    "dist/main.js": {
      "runtime": "@vercel/node@3",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/main.js"
    }
  ]
}
