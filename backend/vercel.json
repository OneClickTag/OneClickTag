{
  "version": 2,
  "buildCommand": "prisma generate --schema=./prisma/schema.prisma && nest build",
  "functions": {
    "dist/main.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/main.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/main.js"
    }
  ]
}
